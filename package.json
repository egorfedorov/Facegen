{
  "name": "@egorfdrv/facegen",
  "version": "0.1.0",
  "type": "module",
  "description": "Generate unique animated faces from any string. Zero dependencies, deterministic, SVG + CSS animations.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./css": "./dist/animations.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && cp src/animations.css dist/animations.css",
    "prepublishOnly": "npm run build",
    "demo": "npx serve demo"
  },
  "keywords": [
    "avatar",
    "generative",
    "svg",
    "face",
    "identicon",
    "deterministic",
    "animated"
  ],
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/react": "^19.0.0"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
